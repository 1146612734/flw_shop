/**app.wxss**/

page {
  background: #f2f2f2;
}

.container {
  box-sizing: border-box;
  font-family: PingFangSC-Light, helvetica, 'Heiti SC';
}

view, image, text, navigator {
  box-sizing: border-box;
  padding: 0;
  margin: 0;
}

view, text {
  font-family: PingFangSC-Light, helvetica, 'Heiti SC';
}

.cmn-font-small {
  color: #999;
  font-size: 24rpx;
}

.cmn-clearfix {
  zoom: 1;
}

.cmn-clearfix:after {
  content: '\0020';
  display: block;
  height: 0;
  font-size: 0;
  clear: both;
  overflow: hidden;
  visibility: hidden;
}

.cmn-ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-wrap: normal;
}

.cmn-two-line {
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box;
  overflow: hidden;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

@font-face {
  font-family: 'iconfont';  /* project id 1376492 */
  src: url('//at.alicdn.com/t/font_1376492_l9ejk9opos.eot');
  src: url('//at.alicdn.com/t/font_1376492_l9ejk9opos.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1376492_l9ejk9opos.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1376492_l9ejk9opos.woff') format('woff'),
  url('//at.alicdn.com/t/font_1376492_l9ejk9opos.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1376492_l9ejk9opos.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

