/**app.wxss**/

page {
  background: #f2f2f2;
}

.container {
  box-sizing: border-box;
  font-family: PingFangSC-Light, helvetica, 'Heiti SC';
}

view, image, text, navigator {
  box-sizing: border-box;
  padding: 0;
  margin: 0;
  color: #333;
}

view, text {
  font-family: PingFangSC-Light, helvetica, 'Heiti SC';
}

.price_unit {
  color: red;
  font-size: 24rpx;
}

.cmn-font-small {
  color: #999;
  font-size: 24rpx;
}

.cmn-clearfix {
  zoom: 1;
}

.cmn-clearfix:after {
  content: '\0020';
  display: block;
  height: 0;
  font-size: 0;
  clear: both;
  overflow: hidden;
  visibility: hidden;
}

.cmn-ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-wrap: normal;
}

.cmn-two-line {
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box;
  overflow: hidden;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.cmn-flex {
  display: -webkit-box; /* 老版本语法: Safari, iOS, Android browser, older WebKit browsers. */
  display: -moz-box; /* 老版本语法: Firefox (buggy) */
  display: -ms-flexbox; /* 混合版本语法: IE 10 */
  display: -webkit-flex; /* 新版本语法: Chrome 21+ */
  display: flex; /* 新版本语法: Opera 12.1, Firefox 22+ */
}

/* 数量选择器 */
.change_num .cell {
  float: left;
  display: block;
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: 1rpx solid #ccc;
  text-align: center;
  box-sizing: border-box;
}
.change_num .cell.disabled {
  color: #999;
  background-color: #eee;
}

